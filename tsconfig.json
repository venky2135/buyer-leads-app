{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM","DOM.Iterable","ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "incremental": true
  },
  "exclude": ["node_modules","./.next"],
  "include": ["next-env.d.ts","**/*.ts","**/*.tsx"]
}
